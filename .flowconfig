[ignore]
.*/__old_tests__/*
.*/dist/.*
.*/node_modules/.*
<PROJECT_ROOT>/benches/.*
<PROJECT_ROOT>/examples/.*

[include]

[libs]

[lints]

all=warn

[options]

include_warnings=true
max_header_tokens=2

module.use_strict=true

suppress_comment=\\(.\\|\n\\)*\\$ExpectError
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
